<%= render 'books/list', books:@books %>

<div class-"col-md-8 offset-md-1">
  <h2>Users</h2>
  <table class="table">
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= image_tag current_user.get_profile_image(100,100) %></td>
        <td><%= link_to "Show",user_path(current_user.id)</td>
      </tr>
    </tbody>
  </table>
</div>
